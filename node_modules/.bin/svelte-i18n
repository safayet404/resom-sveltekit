#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/safayet/Barrzen-Projects/resom-web/node_modules/.pnpm/svelte-i18n@4.0.1_svelte@5.38.3/node_modules/svelte-i18n/dist/node_modules:/home/safayet/Barrzen-Projects/resom-web/node_modules/.pnpm/svelte-i18n@4.0.1_svelte@5.38.3/node_modules/svelte-i18n/node_modules:/home/safayet/Barrzen-Projects/resom-web/node_modules/.pnpm/svelte-i18n@4.0.1_svelte@5.38.3/node_modules:/home/safayet/Barrzen-Projects/resom-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/safayet/Barrzen-Projects/resom-web/node_modules/.pnpm/svelte-i18n@4.0.1_svelte@5.38.3/node_modules/svelte-i18n/dist/node_modules:/home/safayet/Barrzen-Projects/resom-web/node_modules/.pnpm/svelte-i18n@4.0.1_svelte@5.38.3/node_modules/svelte-i18n/node_modules:/home/safayet/Barrzen-Projects/resom-web/node_modules/.pnpm/svelte-i18n@4.0.1_svelte@5.38.3/node_modules:/home/safayet/Barrzen-Projects/resom-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-i18n/dist/cli.js" "$@"
else
  exec node  "$basedir/../svelte-i18n/dist/cli.js" "$@"
fi
